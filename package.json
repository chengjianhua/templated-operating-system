      "editorconfig-tools fix",
      "git add"
    ],
    "src/*.{js,jsx}": [
      "eslint --quiet --fix",
      "git add"
    ],
    "src/*.{css,less,scss,sss}": [
      "stylefmt",
      "stylelint",
      "git add"
    ]
  },
  "scripts": {
    "lint:js": "eslint src tools",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,sss}\"",
